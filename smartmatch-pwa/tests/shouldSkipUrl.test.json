{
    "cases": [
        {
            "id": "no-entry",
            "input": [
                {},
                "https://example.com"
            ],
            "expected": false
        },
        {
            "id": "do-not-retry",
            "input": [
                {
                    "https://example.com": {
                        "doNotRetry": true,
                        "nextRetry": "2020-01-01"
                    }
                },
                "https://example.com"
            ],
            "expected": true
        },
        {
            "id": "future-retry",
            "input": [
                {
                    "https://example.com": {
                        "doNotRetry": false,
                        "nextRetry": "2099-12-31"
                    }
                },
                "https://example.com"
            ],
            "expected": true
        },
        {
            "id": "past-retry",
            "input": [
                {
                    "https://example.com": {
                        "doNotRetry": false,
                        "nextRetry": "2020-01-01"
                    }
                },
                "https://example.com"
            ],
            "expected": false
        },
        {
            "id": "no-do-not-retry-field",
            "input": [
                {
                    "https://example.com": {
                        "nextRetry": "2099-12-31"
                    }
                },
                "https://example.com"
            ],
            "expected": true
        },
        {
            "id": "different-url-not-in-cache",
            "input": [
                {
                    "https://other.com": {
                        "doNotRetry": true,
                        "nextRetry": "2020-01-01"
                    }
                },
                "https://example.com"
            ],
            "expected": false
        },
        {
            "id": "empty-cache-empty-url",
            "input": [
                {},
                ""
            ],
            "expected": false
        }
