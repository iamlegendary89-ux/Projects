{
    "compilerOptions": {
        "target": "es2021",
        "lib": [
            "es2021",
            "webworker"
        ],
        "module": "es2022",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "dist"
    },
    "include": [
        "src",
        "core",
        "data"
    ]
}